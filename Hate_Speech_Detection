      "source": [
        "import pandas as pd\n",
        "\n",
        "# Load your cleaned Reddit dataset\n",
        "file_path = '/content/Reddit_cleaned.csv'  # Adjust the path based on your file location\n",
        "data = pd.read_csv(file_path)\n",
        "\n",
        "# Load the manually labeled data from another CSV file\n",
        "initial_labeled_data_path = '/content/truncated_labelled_data.csv'\n",
        "initial_labeled_data = pd.read_csv(initial_labeled_data_path)\n",
        "\n",
        "# Append initial labeled data to the main dataset for demonstration\n",
        "# In practice, you should manually label a larger subset\n",
        "data = pd.concat([data, initial_labeled_data], ignore_index=True)\n",
        "\n",
        "# Drop rows with missing labels\n",
        "data = data.dropna(subset=['label'])\n",
        "\n",
        "# Ensure labels are integers\n",
        "data['label'] = data['label'].astype(int)\n",
        "\n",
        "# Verify that the data has been correctly labeled\n",
        "print(data.head())\n",
        "print(data['label'].value_counts())"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "# Balancing dataset by oversampling the minority classes\n",
        "class_counts = data['label'].value_counts()\n",
        "max_class_count = class_counts.max()\n",
        "\n",
        "balanced_data = data.copy()\n",
        "for class_value, count in class_counts.items():\n",
        "    if count < max_class_count:\n",
        "        extra_samples = data[data['label'] == class_value].sample(max_class_count - count, replace=True)\n",
        "        balanced_data = pd.concat([balanced_data, extra_samples])\n",
        "\n",
        "# Shuffling the balanced dataset\n",
        "balanced_data = balanced_data.sample(frac=1).reset_index(drop=True)\n",
        "\n",
        "# Verify the class distribution\n",
        "print(balanced_data['label'].value_counts())"
      ],
